{
    "$help": "https://aka.ms/terminal-documentation",
    "$schema": "https://aka.ms/terminal-profiles-schema-preview",
    "actions": 
    [
        {
            "command": 
            {
                "action": "copy",
                "singleLine": false
            },
            "id": "User.copy.644BA8F2",
            "keys": "ctrl+shift+c"
        },
        {
            "command": "unbound",
            "keys": "ctrl+c"
        },
        {
            "command": "unbound",
            "keys": "alt+shift+plus"
        },
        {
            "command": "unbound",
            "keys": "ctrl+shift+d"
        },
        {
            "command": "unbound",
            "keys": "ctrl+v"
        },
        {
            "command": "unbound",
            "keys": "ctrl+tab"
        },
        {
            "command": "unbound",
            "keys": "ctrl+shift+tab"
        },
        {
            "command": "paste",
            "id": "User.paste"
        },
        {
            "command": "duplicateTab",
            "id": "User.duplicateTab",
            "keys": "alt+shift+d"
        },
        {
            "command": "find",
            "id": "User.find",
            "keys": "ctrl+shift+f"
        },
        {
            "command": 
            {
                "action": "splitPane",
                "split": "auto",
                "splitMode": "duplicate"
            },
            "id": "User.splitPane.A6751878"
        },
        {
            "command": 
            {
                "action": "prevTab"
            },
            "id": "User.prevTab.0",
            "keys": "alt+shift+["
        },
        {
            "command": 
            {
                "action": "splitPane",
                "split": "right",
                "splitMode": "duplicate"
            },
            "id": "User.splitPane.FEF0D40B",
            "keys": "alt+shift+\\"
        },
        {
            "command": 
            {
                "action": "nextTab"
            },
            "id": "User.nextTab.0",
            "keys": "alt+shift+]"
        }
    ],
    "copyFormatting": "none",
    "copyOnSelect": false,
    "defaultProfile": "{2c4de342-38b7-51cf-b940-2309a097f518}",
    "firstWindowPreference": "persistedWindowLayout",
    "newTabMenu": 
    [
        {
            "type": "remainingProfiles"
        }
    ],
    "profiles": 
    {
        "defaults": 
        {
            "antialiasingMode": "cleartype",
            "bellStyle": "none",
            "colorScheme": "Custom",
            "cursorShape": "filledBox",
            "experimental.retroTerminalEffect": false,
            "font": 
            {
                "builtinGlyphs": true,
                "colorGlyphs": false,
                "face": "FiraCode Nerd Font",
                "weight": "normal"
            },
            "historySize": 50000,
            "padding": "4"
        },
        "list": 
        [
            {
                "commandline": "%SystemRoot%\\System32\\cmd.exe",
                "guid": "{0caa0dad-35be-5f56-a8ff-afceeeaa6101}",
                "hidden": false,
                "name": "Command Prompt"
            },
            {
                "guid": "{574e775e-4f2a-5b96-ac1e-a2962a402336}",
                "hidden": false,
                "name": "PowerShell",
                "source": "Windows.Terminal.PowershellCore"
            },
            {
                "guid": "{8ab8e6f6-35c6-5b46-8419-3e1568105476}",
                "hidden": false,
                "name": "podman-machine-default",
                "source": "Windows.Terminal.Wsl"
            },
            {{ if not .personalmachine -}}
            {
                "altGrAliasing": true,
                "closeOnExit": "automatic",
                "commandline": "C:\\\\cygwin64\\\\bin\\\\bash.exe -i -l",
                "font": 
                {
                    "size": 12
                },
                "guid": "{2e937641-76b0-40f7-bd57-5c88a5919433}",
                "hidden": false,
                "icon": "https://www.cygwin.com/favicon.ico",
                "name": "Cygwin",
                "snapOnInput": true
            },
            {
                "altGrAliasing": true,
                "closeOnExit": "automatic",
                "commandline": "C:/msys64/msys2_shell.cmd -defterm -here -no-start -msys",
                "font": 
                {
                    "size": 12
                },
                "guid": "{9da2cf80-a0f1-4a10-9b3f-0e5107373e76}",
                "hidden": false,
                "icon": "C:/msys64/msys2.ico",
                "name": "Msys2",
                "snapOnInput": true,
                "startingDirectory": "C:/msys64/home/%USERNAME%"
            },
            {{ end -}}
            {
                "guid": "{2c4de342-38b7-51cf-b940-2309a097f518}",
                "hidden": false,
                "icon": "https://assets.ubuntu.com/v1/49a1a858-favicon-32x32.png",
                "name": "Ubuntu",
                "source": "Windows.Terminal.Wsl"
            }
        ]
    },
    "rendering.graphicsAPI": "direct3d11",
    "schemes": 
    [
        {
            "background": "{{ .colorScheme.base }}",
            "black": "{{ .colorScheme.surface1 }}",
            "blue": "{{ .colorScheme.blue }}",
            "brightBlack": "{{ .colorScheme.surface2 }}",
            "brightBlue": "{{ .colorScheme.blue }}",
            "brightCyan": "{{ .colorScheme.teal }}",
            "brightGreen": "{{ .colorScheme.green }}",
            "brightPurple": "{{ .colorScheme.pink }}",
            "brightRed": "{{ .colorScheme.red }}",
            "brightWhite": "{{ .colorScheme.subtext0 }}",
            "brightYellow": "{{ .colorScheme.yellow }}",
            "cursorColor": "{{ .colorScheme.rosewater }}",
            "cyan": "{{ .colorScheme.teal }}",
            "foreground": "{{ .colorScheme.text }}",
            "green": "{{ .colorScheme.green }}",
            "name": "Custom",
            "purple": "{{ .colorScheme.pink }}",
            "red": "{{ .colorScheme.red }}",
            "selectionBackground": "{{ .colorScheme.surface2 }}",
            "white": "{{ .colorScheme.subtext1 }}",
            "yellow": "{{ .colorScheme.yellow }}"
        }
    ],
    "startOnUserLogin": {{ if .personalmachine -}} false{{ else -}} true{{ end -}},
    "themes": [],
    "windowingBehavior": "useNew"
}
