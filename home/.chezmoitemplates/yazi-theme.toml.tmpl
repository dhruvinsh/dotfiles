[manager]
cwd = { fg = "{{ .colorScheme.teal }}" }

# Hovered
hovered         = { fg = "{{ .colorScheme.base }}", bg = "{{ .colorScheme.blue }}" }
preview_hovered = { underline = true }

# Find
find_keyword  = { fg = "{{ .colorScheme.yellow }}", italic = true }
find_position = { fg = "{{ .colorScheme.pink }}", bg = "reset", italic = true }

# Marker
marker_copied   = { fg = "{{ .colorScheme.green }}", bg = "{{ .colorScheme.green }}" }
marker_cut      = { fg = "{{ .colorScheme.red }}", bg = "{{ .colorScheme.red }}" }
marker_selected = { fg = "{{ .colorScheme.blue }}", bg = "{{ .colorScheme.blue }}" }

# Tab
tab_active   = { fg = "{{ .colorScheme.base }}", bg = "{{ .colorScheme.text }}" }
tab_inactive = { fg = "{{ .colorScheme.text }}", bg = "{{ .colorScheme.surface1 }}" }
tab_width    = 1

# Count
count_copied   = { fg = "{{ .colorScheme.base }}", bg = "{{ .colorScheme.green }}" }
count_cut      = { fg = "{{ .colorScheme.base }}", bg = "{{ .colorScheme.red }}" }
count_selected = { fg = "{{ .colorScheme.base }}", bg = "{{ .colorScheme.blue }}" }

# Border
border_symbol = "│"
border_style  = { fg = "{{ .colorScheme.overlay1 }}" }

# Highlighting
syntect_theme = "~/.config/yazi/Catppuccin-mocha.tmTheme"

[status]
separator_open  = ""
separator_close = ""
separator_style = { fg = "{{ .colorScheme.surface1 }}", bg = "{{ .colorScheme.surface1 }}" }

# Mode
mode_normal = { fg = "{{ .colorScheme.base }}", bg = "{{ .colorScheme.blue }}", bold = true }
mode_select = { fg = "{{ .colorScheme.base }}", bg = "{{ .colorScheme.green }}", bold = true }
mode_unset  = { fg = "{{ .colorScheme.base }}", bg = "{{ .colorScheme.flamingo }}", bold = true }

# Progress
progress_label  = { fg = "{{ .colorScheme.white }}", bold = true }
progress_normal = { fg = "{{ .colorScheme.blue }}", bg = "{{ .colorScheme.surface1 }}" }
progress_error  = { fg = "{{ .colorScheme.red }}", bg = "{{ .colorScheme.surface1 }}" }

# Permissions
permissions_t = { fg = "{{ .colorScheme.blue }}" }
permissions_r = { fg = "{{ .colorScheme.yellow }}" }
permissions_w = { fg = "{{ .colorScheme.red }}" }
permissions_x = { fg = "{{ .colorScheme.green }}" }
permissions_s = { fg = "{{ .colorScheme.overlay1 }}" }

[input]
border   = { fg = "{{ .colorScheme.blue }}" }
title    = {}
value    = {}
selected = { reversed = true }

[select]
border   = { fg = "{{ .colorScheme.blue }}" }
active   = { fg = "{{ .colorScheme.pink }}" }
inactive = {}

[tasks]
border  = { fg = "{{ .colorScheme.blue }}" }
title   = {}
hovered = { underline = true }

[which]
mask            = { bg = "{{ .colorScheme.surface0 }}" }
cand            = { fg = "{{ .colorScheme.teal }}" }
rest            = { fg = "{{ .colorScheme.overlay2 }}" }
desc            = { fg = "{{ .colorScheme.pink }}" }
separator       = "  "
separator_style = { fg = "{{ .colorScheme.surface2 }}" }

[help]
on      = { fg = "{{ .colorScheme.pink }}" }
exec    = { fg = "{{ .colorScheme.teal }}" }
desc    = { fg = "{{ .colorScheme.overlay2 }}" }
hovered = { bg = "{{ .colorScheme.surface2 }}", bold = true }
footer  = { fg = "{{ .colorScheme.surface1 }}", bg = "{{ .colorScheme.text }}" }

[filetype]

rules = [
	# Media
	{ mime = "image/*", fg = "{{ .colorScheme.teal }}" },
	{ mime = "{audio,video}/*", fg = "{{ .colorScheme.yellow }}" },

	# Archives
  { mime = "application/{,g}zip", fg = "{{ .colorScheme.pink }}" },
	{ mime = "application/x-{tar,bzip*,7z-compressed,xz,rar}", fg = "{{ .colorScheme.pink }}" },

	# Fallback
	{ name = "*", fg = "{{ .colorScheme.text }}" },
	{ name = "*/", fg = "{{ .colorScheme.blue }}" }
]

